{
  "name": "svelte_socket_starter",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "prod": "node dist/main.js",
    "start": "concurrently npm:dev-server:*",
    "dev-server:compile": "tsc -w",
    "dev-server:run": "nodemon"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/",
      "coverage/"
    ],
    "watch": [
      "./"
    ],
    "exec": "node --loader ts-node/esm main.ts --emit",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "dependencies": {
    "@lottiefiles/svelte-lottie-player": "^0.2.0",
    "@project-serum/anchor": "^0.23.0",
    "@solana/spl-token": "^0.2.0",
    "bs58": "^5.0.0",
    "express": "^4.17.1",
    "firebase": "^9.6.10",
    "socket-io": "^1.0.0",
    "socket.io": "^4.1.2",
    "svelte-spa-router": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-json": "^4.1.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/express": "^4.17.12",
    "@types/node": "^15.14.9",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^7.1.0",
    "esbuild-loader": "^2.18.0",
    "jest": "^27.0.4",
    "mini-css-extract-plugin": "^2.6.0",
    "mini-html-webpack-plugin": "^3.1.3",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "rollup-plugin-node-globals": "^1.4.0",
    "sirv-cli": "^1.0.12",
    "socket.io-client": "^4.1.2",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "terser-webpack-plugin": "^5.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.3.2",
    "webpack": "^5.70.0",
    "webpack-browser-extension-polyfill": "^1.1.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-plugin-serve": "^1.6.0",
    "webpackbar": "^5.0.2"
  }
}
